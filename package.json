{
  "name": "skyrim-effect",
  "version": "0.0.1",
  "private": false,
  "type": "module",
  "main": "./dist/skyrim-effect.umd.js",
  "module": "./dist/skyrim-effect.es.js",
  "types": "./dist/dts/index.d.ts",
  "scripts": {
    "build": "vite build && tsc -b --emitDeclarationOnly",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "doc": "typedoc",
    "test": "vitest --no-watch",
    "coverage": "vitest --coverage --no-watch",
    "make-badges": "istanbul-badges-readme"
  },
  "keywords": [
    "skyrim",
    "skryim platform"
  ],
  "peerDependencies": {
    "@skyrim-platform/skyrim-platform": "2.9.0",
    "effect": "^3.12.1"
  },
  "devDependencies": {
    "typescript": "^5.7.3",
    "typedoc": "^0.27.6",
    "vite": "^6.0.7",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8",
    "@vitest/coverage-v8": "^2.1.8",
    "@effect/vitest": "^0.16.1",
    "@stylistic/eslint-plugin": "^2.12.1",
    "eslint": "^9.18.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-config-prettier": "^9.1.0",
    "@eslint/js": "^9.18.0",
    "prettier": "^3.4.2",
    "typescript-eslint": "^8.19.1",
    "husky": "^9.1.7"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/dts/index.d.ts",
      "import": "./dist/skyrim-effect.es.js",
      "default": "./dist/skyrim-effect.umd.js"
    },
    "./common/Debug": {
      "types": "./dist/dts/common/Debug.d.ts"
    },
    "./common/Error": {
      "types": "./dist/dts/common/Error.d.ts"
    },
    "./common/Log": {
      "types": "./dist/dts/common/Log.d.ts"
    },
    "./common/Type": {
      "types": "./dist/dts/common/Type.d.ts"
    },
    "./game/Event": {
      "types": "./dist/dts/game/Event.d.ts"
    },
    "./game/Form": {
      "types": "./dist/dts/game/Form.d.ts"
    }
  }
}

{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "noEmit": false,
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "lib": [
      "ESNext",
      "DOM"
    ],
    "declaration": true,
    "declarationMap": true,
    "strict": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "forceConsistentCasingInFileNames": true,
    "stripInternal": true,
    "skipLibCheck": true,
    "types": [
      "node",
      "vitest/globals"
    ],
    "baseUrl": "./",
    "rootDir": "../",
    "outDir": "../dist"
  },
  "include": [
    "./"
  ]
}
